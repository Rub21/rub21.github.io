{
  "name": "rub21.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "content": "rm -rf .contentlayer/ && yarn contentlayer build --verbose",
    "dev": "next dev",
    "build": "NODE_ENV=production yarn content && NODE_ENV=production next build",
    "start": "yarn content && next start",
    "clean": "rimraf build/",
    "lint": "next lint",
    "deploy": "yarn content && yarn clean && PUBLIC_URL=/ REACT_APP_ENV=production  yarn build && gh-pages -d out",
    "prettier": "yarn content && yarn clean && prettier --write 'src/**/*.tsx'"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.1",
    "@types/node": "20.5.3",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "contentlayer": "^0.3.4",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "framer-motion": "^10.16.1",
    "next": "13.4.19",
    "next-contentlayer": "^0.3.4",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.10.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.14.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "typewriter-effect": "^2.20.1"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.1"
  }
}
